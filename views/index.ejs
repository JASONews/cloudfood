                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title"><i class="fa fa-long-arrow-right fa-fw"></i> New Order</h3>
                        </div>
                        <div class="panel-body">
                            <form id='orderForm' class="form-horizontal">
                                <!-- Text input-->
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="textinput">Name</label>
                                    <div class="col-md-8">
                                        <input id="name"  placeholder="Enter your name" class="form-control input-md" required type="text">

                                    </div>
                                </div>

                                <div class = "form-group">
                                    <label class="col-md-4 control-label" for="textinput">Address</label>

                                    <div id="locationField" class="col-md-8">
                                        <input id="autocomplete" class="form-control input-md" required type="text" placeholder="Enter your address"
                                               onFocus="geolocate()" type="text"></input>
                                    </div>

                                </div>


                                <!-- Text input-->
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="textinput">Restaurant</label>
                                    <div id="locationField" class="col-md-8">
                                        <input id="autocomplete2" class="form-control input-md"required type="text" placeholder="Enter your desired restaurant"
                                               onFocus="geolocate()" type="text"></input>


                                    </div>
                                </div>
                                <!-- Textarea -->
                                <div class="form-group">
                                    <label class="col-md-4 control-label" for="textarea">Order Entry</label>
                                    <div class="col-md-8">
                                        <textarea class="form-control" id="order" name="textarea" required></textarea>



                                    </div>

                                </div>
                                <div class="form-group">
                                    <label class='col-md-4 control-label'>Total cost and tip</label>
                                    <div class="col-md-8">

                                        <div class="input-group">
                                            <span class="input-group-addon">$</span>
                                            <input id='cost' type="text" class="form-control" aria-label="Amount (to the nearest dollar)">
                                        </div>
                                        <br>
                                        <label><input type ='radio' name='tip'> 10%</label>
                                        <label><input type ='radio' name='tip'> 20%</label>
                                        <label><input type ='radio' name='tip'> 30%</label>
                                        <label id='custom'><input type='radio' name = 'tip'><input id ='customTip' style='width:40px' type = 'text'></label>
                                        <br>

                                        <button type='button' class='btn btn-success' id='placeOrder'>Place Order</button>



                                    </div>
                            </form>

                            </div>
                        </div>
                    </div>
